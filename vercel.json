{
    "builds": [
        {
            "src": "api/proxy.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/playlist.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/proxy",
            "dest": "/api/proxy.py"
        },
        {
            "src": "/api/playlist",
            "dest": "/api/playlist.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}